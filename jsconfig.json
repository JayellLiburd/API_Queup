{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "Node",
        "target": "ES2020",
        "jsx": "react",
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "typeRoots": ["@types", "node_modules/@types"],
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}